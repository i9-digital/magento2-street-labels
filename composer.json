{
    "name": "i9/magento2-streetlabel",
    "description": "Show individual labels to street fields on checkout",
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.3",
        "magento/module-checkout": ">=100.3.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/i9-digital/magento2-street-labels"
        }
    ],
    "autoload": {
        "psr-4": {
            "I9\\StreetLabel\\": ""
        },
        "files": [
            "registration.php"
        ]
    }
}
